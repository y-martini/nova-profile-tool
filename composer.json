{
    "name": "yuriy-martini/nova-profile-tool",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/view": "5.*|^6.0|^7.0|^8.0",
        "laravel/nova": "~2.0|~3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "YuriyMartini\\Nova\\Tools\\Profile\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YuriyMartini\\Nova\\Tools\\Profile\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
